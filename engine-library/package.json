{
  "name": "@concept-design/engine",
  "version": "1.0.0",
  "description": "Concept Design Engine for Next.js and TypeScript applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "concept-design",
    "nextjs",
    "typescript",
    "architecture",
    "framework",
    "engine"
  ],
  "author": "Concept Design Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sonnenreich/concept.git",
    "directory": "engine-library"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=18.0.0",
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "@prisma/client": {
      "optional": true
    }
  },
  "dependencies": {
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "@prisma/client": "^5.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
